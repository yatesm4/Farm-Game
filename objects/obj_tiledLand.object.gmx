            <string>/// Player Interaction
var gm = obj_game_manager;
var i, items, item, item_count, sel;
var can = false;
draw_self();

///Player Interaction

var dist = point_distance(obj_Player.phy_position_x,obj_Player.phy_position_y,x,y);

if(dist &lt; 8 &amp;&amp; obj_Player.action == true &amp;&amp; seeded == false){
    // Check players inventory
    sel = obj_Player.select_item;
    
    show_debug_message("Interaction -&gt; tiledLand");
    show_debug_message("Selected item slot: " + string(sel));
    
    items = ds_map_size(gm.save_inv);
    item = ds_map_find_first(gm.save_inv);
    
    for(i=0;i&lt;sel;i+=1)
    {
        if(i == (sel - 1))
        {
            item_count = ds_map_find_value(gm.save_inv,item);
            if(item == "obj_Seeds" &amp;&amp; item_count &gt; 0)
            {
                show_debug_message("Seeds: TRUE");
                show_debug_message("Count: " + string(item_count));
                item_count -= 1;
                if(item_count == 0)
                    ds_map_delete(gm.save_inv,item);
                else
                    ds_map_replace(gm.save_inv,item,item_count);
                
                show_debug_message("New Count: " + string(item_count));
                instance_create(x,y,obj_Wheat);
                show_debug_message("Crop planted");
                seeded = true;

            }
        }
        else
        {
            item = ds_map_find_next(gm.save_inv,item);
        }
    }
}
</string>
