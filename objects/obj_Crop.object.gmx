        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Player Interaction

randomize();

var m = irandom_range(1,3);
var n = irandom_range(1,2);
var o = irandom_range(1,4);
var i;
draw_self();

// grab distance between player and object
var dist = point_distance(obj_Player.phy_position_x,obj_Player.phy_position_y,x,y);

// if char within 24 pixels
if(dist &lt; 24){
    
    // if characters alt (x) action is true
    if(obj_Player.alt_action == true){
    
        // if the state (stage) is less than 5 (fully grown)
        if (image_index &lt; grown)
        {
            // drop a random seed amount between 1 and 2
            for(i=0;i&lt;n;i+=1)
            {
                instance_create(x,y,seed_dropped);   
            }
        }
        // if the plant is fully grown
        else
        {
            // drop a random wheat amount between 1 and 3
            for(i=0;i&lt;m;i+=1)
            {
                instance_create((x-5),y,crop_dropped);
            }
            // drop a random seed amount between 1 and 4
            for(i=0;i&lt;o;i+=1)
            {
                instance_create((x+5),y,seed_dropped);   
            }
        }
        // get the tiled land object directly under the now harvested wheat
        tiled = instance_position(x,y,obj_tiledLand);
        if(tiled){
            // set the tiled land's seeded attribute to false
            tiled.seeded = false;
        }
        // destroy the plant object=
        instance_destroy();
        show_debug_message("Crop destroyed");
    }
}
</string>
